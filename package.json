    "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/coverage/**",
    "!**/node_modules/**",
    "!**/.expo/**",
    "!**/babel.config.js",
    "!**/jest.setup.js"
  ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
